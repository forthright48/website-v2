extends ../layout.pug

block content
    .item.u-fill-view.u-flex-top
        .box
            .any.item
                .u-text-center
                    a(href="/gateway/getChildren/"+data.parentId)
                        i.fa.fa-level-up(aria-hidden="true")
                        span Go Up
                .markdown-body
                    h1= data.name
                    span !{data.body}
