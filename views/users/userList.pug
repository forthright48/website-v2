extends ../layout.pug

block content
    .items.u-fill-view.u-text-center
        h1 User List
        .box
            .any.items
                table(align="center")
                    thead
                        th Index
                        th Username
                        th Activated
                    tbody
                        each val,key in data
                            tr
                                td= key+1
                                td= val.username
                                td= val.activated
